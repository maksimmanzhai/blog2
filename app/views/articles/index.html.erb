<% @articles.each do |article| %>
  
  <h2>
    <%= article.title %>
  </h2>

  <p>
    <%= article.text %>
  </p>
  
  <h5>
    <%= article.created_at %>
  </h5>

  <p>
    <%= link_to 'Show', article_path(article) %> | 
    <%= link_to 'Edit', edit_article_path(article) %> | 
    <%= link_to 'Delete', article_path(article), method: :delete %>
      
    </p>

<% end %>
